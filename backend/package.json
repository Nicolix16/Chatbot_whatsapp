{
  "name": "base-bailey-json",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config ./dist/app.js dotenv_config_path=.env",
    "start:dashboard": "node -r dotenv/config ./dist/server.js dotenv_config_path=.env",
    "lint": "eslint src --ext .ts",
    "dev": "npm run lint && nodemon ./src/app.ts --env-file=.env",
    "dev:dashboard": "tsx --env-file=.env ./src/server.ts",
    "build": "npx rollup -c",
    "build:server": "npx tsc -p tsconfig.json",
    "vercel-build": "npm run build:server",
    "seed:user": "tsx --env-file=.env ./src/scripts/seedUser.ts",
    "seed:pedidos": "tsx --env-file=.env ./src/scripts/seedPedidos.ts",
    "seed:pedidos:delete": "tsx --env-file=.env ./src/scripts/seedPedidos.ts --delete",
    "migrate:clientes": "tsx --env-file=.env ./src/scripts/migrateClientes.ts",
    "crear:encargado-hogares": "tsx --env-file=.env ./src/scripts/crearEncargadoHogares.ts",
    "verificar:usuarios": "tsx --env-file=.env ./src/scripts/verificarUsuariosNotificaciones.ts",
    "probar:notificaciones": "tsx --env-file=.env ./src/scripts/probarNotificaciones.ts"
  },
  "keywords": [],
  "dependencies": {
    "@builderbot/bot": "1.3.2-y.0",
    "@builderbot/database-mongo": "^1.3.5",
    "@builderbot/provider-meta": "1.3.2-y.0",
    "@sendgrid/mail": "^8.1.6",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^7.0.0",
    "mongoose": "^8.19.4",
    "multer": "^2.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/connect": "^3.4.38",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^8.52.0",
    "eslint-plugin-builderbot": "latest",
    "nodemon": "^3.1.0",
    "rollup": "^4.10.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "author": "",
  "license": "ISC"
}
